{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https: http:" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}